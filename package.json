    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "clean": "yarn run rimraf **/dist",
        "build": "yarn clean && yarn tsc -b",
        "test": "yarn test-core && yarn test-awskinesis && yarn test-testrail && yarn test-ui && yarn test-browsers && yarn test-mobile-apps && yarn test-web-services && yarn test-html",
