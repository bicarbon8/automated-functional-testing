        "test-html": "yarn workspace aft-logging-html test",
        "test-examples": "yarn workspace aft-examples test"
    },
    "dependencies": {
        "lerna": "^4.0.0"
    },
    "packageManager": "yarn@3.2.0",
    "devDependencies": {
        "jasmine-ts": "^0.4.0",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4"
    }
