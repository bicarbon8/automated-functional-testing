{
  "name": "automated-functional-testing",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jason Holt Smith  <bicarbon8@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "yarn run rimraf ./packages/**/dist && yarn run rimraf ./packages/**/coverage && yarn run rimraf ./packages/**/.nyc_output",
    "build": "yarn clean && yarn tsc -b",
    "test": "nx run-many --target=test --projects=aft-core,aft-logging-awskinesis,aft-logging-html,aft-testrail,aft-ui,aft-ui-browsers,aft-ui-mobile-apps,aft-web-services",
    "coverage": "nx run-many --target=coverage --projects=aft-core,aft-logging-awskinesis,aft-logging-html,aft-testrail,aft-ui,aft-ui-browsers,aft-ui-mobile-apps,aft-web-services",
    "test-core": "nx run aft-core:test",
    "test-awskinesis": "nx run aft-logging-awskinesis:test",
    "test-testrail": "nx run aft-testrail:test",
    "test-ui": "nx run aft-ui:test",
    "test-browsers": "nx run aft-ui-browsers:test",
    "test-mobile-apps": "nx run aft-ui-mobile-apps:test",
    "test-web-services": "nx run aft-web-services:test",
    "test-html": "nx run aft-logging-html:test",
    "test-examples": "nx run aft:examples test"
  },
  "dependencies": {
    "lerna": "^4.0.0"
  },
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "@nrwl/nx-cloud": "latest",
    "node": "^17.7.2",
    "nx": "14.1.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  }
}
