  "bugs": {
    "url": "https://github.com/bicarbon8/automated-functional-testing/issues"
  },
  "homepage": "https://github.com/bicarbon8/automated-functional-testing#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "lodash": "^4.17.21",
    "ts-simple-nameof": "^1.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "@types/node": "^15.0.1",
    "@types/uuid": "^8.3.0",
    "jasmine": "^3.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  }
