{
    "LogManager": {
        "level": "info",
        "plugins": [{
            "name": "testrail-logging-plugin",
            "searchDirectory": "../",
            "options": {
                "level": "warn",
                "enabled": false
            }
        },{
            "name": "kinesis-logging-plugin",
            "searchDirectory": "../",
            "options": {
                "level": "trace",
                "batch": true,
                "batchSize": 10,
                "enabled": false
            }
        },{
            "name": "html-logging-plugin",
            "searchDirectory": "../",
            "options": {
                "level": "warn"
            }
        }]
    },
    "TestCaseManager": {
        "plugins": [{
            "name": "testrail-test-case-plugin",
            "searchDirectory": "../",
            "options": {
                "enabled": false
            }
        }]
    },
    "TestRailConfig": {
        "url": "%testrail_url%",
        "user": "%testrail_user%",
        "access_key": "%testrail_pass%",
        "project_id": 3,
        "suite_ids": [744]
    },
    "BrowserStackConfig": {
        "user": "%browserstack_user%",
        "key": "%browserstack_key%",
        "debug": true
    },
    "BrowserSessionGeneratorManager": {
        "plugins": [{
            "name": "browserstack-browser-session-generator-plugin",
            "searchDirectory": "../",
            "options": {
                "uiplatform": "windows_10_chrome"
            }
        }]
    },
    "MobileAppSessionGeneratorManager": {
        "plugins": [{
            "name": "browserstack-mobile-app-session-generator-plugin",
            "searchDirectory": "../",
            "options": {
                "uiplatform": "android_11_+_+_Google Pixel 5",
                "remoteOptions": {
                    "logLevel": "silent"
                }
            }
        }]
    }
}