{
  "name": "aft-ui-browsers",
  "version": "5.0.1",
  "description": "Automated Functional Testing (AFT) package supporting UI testing in browsers with support for BrowserStack, Sauce Labs and Local Selenium Grid",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "yarn clean && tsc --build",
    "pretest": "yarn build",
    "test": "jasmine ./dist/test/**/*-spec.js",
    "coverage": "nyc yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicarbon8/automated-functional-testing.git"
  },
  "keywords": [
    "aft",
    "automation",
    "e2e",
    "integration",
    "functional",
    "ui",
    "browser",
    "selenium",
    "browserstack",
    "saucelabs",
    "seleniumgrid"
  ],
  "author": "Jason Holt Smith <bicarbon8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bicarbon8/automated-functional-testing/issues"
  },
  "homepage": "https://github.com/bicarbon8/automated-functional-testing#readme",
  "dependencies": {
    "aft-ui": "^5.0.1",
    "selenium-webdriver": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "@types/node": "^15.0.1",
    "@types/selenium-webdriver": "^4.0.12",
    "jasmine": "^3.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
}
