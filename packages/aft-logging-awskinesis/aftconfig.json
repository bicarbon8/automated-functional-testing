{
    "logmanager": {
        "pluginNames": ["kinesis-logging-plugin"],
        "searchDir": "../"
    },
    "kinesisloggingplugin": {
        "level": "warn",
        "accessKeyId": "set_your_access_key_id_here",
        "secretAccessKey": "set_your_secret_access_key_here",
        "sessionToken": "set_your_session_token_here",
        "region": "us-west-1",
        "deliveryStream": "aft-reporting",
        "batch": true,
        "batchSize": 4
    }
}