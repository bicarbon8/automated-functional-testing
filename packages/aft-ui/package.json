  "dependencies": {
    "aft-core": "^5.0.1"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.10",
    "@types/node": "^15.0.1",
    "jasmine": "^3.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  }
